{
    "private": true,
    "type": "module",
    "scripts": {
        "start": "NODE_ENV=production node server.js",
        "server": "nodemon server.js",
        "client": "vite",
        "dev": "node start-dev.js",
        "dev:legacy": "concurrently \"npm run server\" \"npm run client\" \"npm run supabase:check\"",
        "build": "vite build",
        "prod": "node start-prod.js",
        "prod:vercel": "vercel deploy --prod",
        "prod:setup": "node -e \"const fs = require('fs'); if(!fs.existsSync('.env.production')) { fs.copyFileSync('.env', '.env.production'); console.log('Created .env.production'); }\" && npm run build",
        "vercel-build": "vite build && node vercel-setup.js",
        "render-build": "node render-setup.js",
        "postinstall": "npm install @vitejs/plugin-react vite --no-save || true",
        "supabase": "node start-app.js",
        "supabase:check": "node -e \"const { exec } = require('child_process'); exec('node setup-supabase-tables.js', (error, stdout, stderr) => { console.log(stdout); if (error) { console.error('Error checking Supabase tables:', error); } });\"",
        "deploy": "npm run build && npm run start"
    },
    "devDependencies": {
        "@headlessui/react": "^2.0.0",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-react": "^4.2.0",
        "autoprefixer": "^10.4.12",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.2.0",
        "nodemon": "^3.1.0",
        "postcss": "^8.4.31",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.2.1"
    },
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "@supabase/supabase-js": "^2.39.0",
        "@vitejs/plugin-react": "^4.2.0",
        "bcryptjs": "^2.4.3",
        "bootstrap": "^5.3.5",
        "cors": "^2.8.5",
        "dotenv": "^16.4.1",
        "express": "^4.18.3",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.487.0",
        "node-fetch": "^3.3.2",
        "puppeteer": "^24.6.1",
        "react-bootstrap": "^2.10.9",
        "react-datepicker": "^8.3.0",
        "react-helmet": "^6.1.0",
        "react-helmet-async": "^2.0.5",
        "react-icons": "^5.5.0",
        "react-router-dom": "^7.5.0",
        "resend": "^4.3.0",
        "vite": "^6.0.11"
    }
}
